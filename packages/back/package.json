{
  "name": "back",
  "version": "1.0.0",
  "description": "Apollo + Express + MongoDB chat server",
  "author": "Mart√≠n Montes <martin11lrx@gmail.com>",
  "license": "MIT",
  "main": "index.js",
  "private": false,
  "engines": {
    "node": ">=8.0.0",
    "npm": ">=5.0.0"
  },
  "scripts": {
    "start": "cross-env NODE_ENV=development nodemon src/index.js --exec babel-node",
    "lint": "eslint . --fix",
    "pretest": "npm run lint",
    "test": "cross-env NODE_ENV=test nyc mocha --timeout 10000 test/",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "clean": "rm -rf dist/",
    "prebuild": "npm run clean",
    "build": "babel -d dist/ src/",
    "production": "cross-env NODE_ENV=production node index.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mmontes11/back"
  },
  "keywords": [
    "apollo",
    "apollo-server",
    "express",
    "mongo",
    "mongodb",
    "mongoose",
    "chat"
  ],
  "dependencies": {
    "apollo-server": "^2.6.9",
    "apollo-server-express": "^2.6.9",
    "bcrypt": "^3.0.6",
    "bluebird": "^3.4.7",
    "body-parser": "^1.18.3",
    "cors": "2.8.1",
    "cross-env": "^5.1.4",
    "dataloader": "^1.4.0",
    "dotenv": "^8.0.0",
    "express": "^4.17.3",
    "express-jwt": "^5.3.1",
    "express-winston": "^3.0.0",
    "graphql": "^14.4.2",
    "graphql-resolvers": "^0.3.3",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.7.0",
    "validator": "^11.1.0",
    "winston": "^3.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.5.5",
    "@babel/core": "^7.1.6",
    "@babel/node": "^7.0.0",
    "@babel/preset-env": "^7.1.6",
    "@babel/register": "^7.0.0",
    "babel-core": "^7.0.0-bridge.0",
    "eslint-config-mmontes11-node": "^1.0.5",
    "nodemon": "^1.18.11"
  }
}
